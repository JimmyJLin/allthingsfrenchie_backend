<% layout('cms') -%>

<div id="profile_form" class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
 <h1 class="page-header">Update Profile</h1>

 <form method="post">

   <div class="form-group">
     <label="name">Name </label>
     <input type="text" class="form-control" name="name">
   </div>

   <div class="form-group">
     <label class="btn btn-primary btn-file">
       click here to upload photo <input type="file" style="display: none;" name="file">
     </label>
   </div>

   <div class="form-group">
     <label="text"> Photo URL </label>
     <input type="text" class="form-control" name="photoURL">
   </div>

   <button type="submit" class="btn btn-success">Submit</button>

 </form>


</div>
